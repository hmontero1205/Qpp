<div id="qe-<%= queue_entry.id %>"class="card border-info queue_entry" style="width: 18rem;" start_time="<%= queue_entry.created_at %>">
  <div class="card-header bg-dark" style="color: white">
    <span><%= queue_entry.student %></span>
    <button class="qe-btn btn btn-danger" style="float: right; padding: 5px 10px; margin: -5px;">X</button>
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item"></li>
    <li class="list-group-item"><%= queue_entry.description %></li>
  </ul>
</div>
