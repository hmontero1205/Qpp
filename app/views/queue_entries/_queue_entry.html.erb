<div id="qe-<%= queue_entry.id %>"class="card border-info queue_entry" start_time="<%= queue_entry.created_at.strftime("%Y-%m-%dT%H:%M:%S+00:00") %>">
  <div class="card-header bg-dark" style="color: white">
    <span><%= queue_entry.student %></span>
    <span></span>
    <span style="display: inline-block; float: right">
      <%# <button id="thread-btn-<%= queue_entry.id %1>" class="btn thread-btn btn-info" style="margin: -5px 5px -5px -5px;"> %>
      <%#   <%= " (+#{Chat.where(queue_entry_id: queue_entry.id).length})" %1> %>
      <%# </button> %>
      <button id="thread-btn-<%= queue_entry.id %>" class="btn thread-btn btn-info" style="">
        <%= "+#{Chat.where(queue_entry_id: queue_entry.id).length}" %>
      </button>
      <% if ((user_signed_in? and current_user == @oh.user) or (queue_entry.creator_id.eql? session.id.to_s)) %>
        <button class="btn qe-btn btn-danger">X</button>
      <% end %>
    </span>
  </div>
  <ul class="list-group list-group-flush">
    <%# <li class="list-group-item"></li> %>
    <li class="list-group-item">
      <%= queue_entry.description %>
      
    </li>
  </ul>
</div>
